{
  "name": "react-cloud",
  "version": "1.0.0",
  "description": "A soundcloud client built with React",
  "homepage": "https://github.com/JayKan/react-cloud#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JayKan/react-cloud.git"
